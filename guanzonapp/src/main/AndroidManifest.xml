<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.guanzongroup.com.guanzonapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GApps">
        <activity
            android:name="org.guanzongroup.com.guanzonapp.Activity_SplashScreen"
            android:exported="true"
            android:screenOrientation="userPortrait"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.GApps.NoActionBar" />
        <activity
            android:name="org.guanzongroup.com.guanzonapp.Activity_Dashboard"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>